<mapper namespace="com.itbank.model.BoardDAO">

	<select id="selectAll"
			resultType="com.itbank.vo.BoardVO">
		
		select * from board_view
		    order by idx desc
		    offset 0 rows
		    fetch first 10 rows only
	</select>

</mapper>