package Service;

import java.util.Random;

import org.springframework.stereotype.Service;

@Service
public class TestService {
	public int total(int n) {
		int sum=0;
		
		for (int i=0; i<=n; i++) {
			sum+=i;
		}
		return sum;
	}
	
	public int lotto() {
		int answer[];
		
		
		for (int i=0; i<6; i++) {
			int value=(int)(Math.random()*46)+1;
			answer.add(value);
		}
		
		
		return 0;
	}
}
