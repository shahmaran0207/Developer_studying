package Service;

import java.util.Random;

import org.springframework.stereotype.Service;

@Service
public class TestService {
	public int total(int n) {
		int sum=0;
		
		for (int i=0; i<=n; i++) {
			sum+=i;
		}
		return sum;
	}
	
	public int lotto() {
    	ArrayList<String> list = new ArrayList<String>();
		
